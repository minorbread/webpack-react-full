{
  "name": "webpack-react",
  "version": "1.0.0",
  "description": "full",
  "main": "index.js",
  "scripts": {
    "build:client": "webpack --config ./build/webpack.config.client.js",
    "build:server": "webpack --config ./build/webpack.config.server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.client.js",
    "clear": "rimraf dist",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "start": "node server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minorbread/webpack-react-full.git"
  },
  "author": "zgq",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/minorbread/webpack-react-full/issues"
  },
  "homepage": "https://github.com/minorbread/webpack-react-full#readme",
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "webpack": "^4.8.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.5",
    "html-webpack-plugin": "^3.2.0",
    "react-hot-loader": "^4.2.0",
    "rimraf": "^2.6.2",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
